<div id="measure-tab">
  <h3 style="float: left;">Entering Counts</h3>
  <div id="count-buttons" style="float: left; margin-left: 6px">
    <h6><a id="cancel-counting-button" class="btn btn-danger">Cancel Counting</a></h6>
  </div>
  <div id="count-session-entry">
    <button id="count-minus-one-button" class="btn btn-danger large">&ndash; 1</button>
    <div id="timer">
      <h6 id="minutes"></h6>
      <h6 id="seconds"></h6>
    </div>
    <div id="counter">0 <h6>people</h6></div>
    <button id="count-plus-one-button" class="btn btn-primary large">+ 1</button>
    <button id="count-plus-five-button" class="btn btn-primary large">+ 5</button>
  </div>
</div>